{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "jsx": "react",
    "pretty": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "its-fine": ["./src/index.js"]
    }
  },
  "include": ["src/**/*"]
}
